<table class="table">
  <tr>
    <td>管理員編號</td>
    <td><%= @user.id %></td>
  </tr>

  <tr>
    <td>姓名</td>
    <td><%= @user.fullname %></td>
  </tr>

  <tr>
    <td>email</td>
    <td><a href="mailto:<%= @user.email %>" > <%= @user.email %></td></a>
  </tr>

  <tr>
    <td>性別</td>
    <td><%= @user.profile.try(:gender) == 1 ? '男性' : '女性' %></td>
  </tr>

  <tr>
    <td>生日</td>
    <td><%= @user.profile.try(:dob) %></td>
  </tr>

  <tr>
    <td>教育程度</td>
    <td><%= @user.profile.try(:education) %></td>
  </tr>

  <tr>
    <td>國籍</td>
    <td><%= @user.profile.try(:country) %></td>
  </tr>

  <tr>
    <td>戶籍地城市</td>
    <td><%= @user.profile.try(:register_address) %></td>
  </tr>

  <tr>
    <td>居住地城市</td>
    <td><%= @user.profile.try(:address) %></td>
  </tr>

  <tr>
    <td>聯絡電話</td>
    <td><%= @user.profile.try(:phone) %></td>
  </tr>


 <tr>
    <td>行動電話</td>
    <td><%= @user.profile.try(:cellphone) %></td>
  </tr>

</table>

<%= link_to "返回",:back %>